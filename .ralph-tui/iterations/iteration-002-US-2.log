ontent\", \"cta\"] as const).map((type) => (\n            <div\n              key={type}\n              className={`aspect-square rounded-sm ${getGradientClasses(template, type)} flex items-center justify-center`}\n            >\n              <span\n                className={`text-[8px] font-medium ${getTextColorClass(template)}`}\n              >\n                {type === \"title\" ? \"T\" : type === \"content\" ? \"C\" : \"CTA\"}\n              </span>\n            </div>\n          ))}\n        </div>\n\n        {/* Live Preview with User Content */}\n        {previewSlide && (\n          <div\n            className={`aspect-[4/3] rounded-md p-3 flex flex-col justify-center items-center text-center ${getGradientClasses(template, previewSlide.type)}`}\n          >\n            {previewSlide.emoji && (\n              <span className=\"text-lg mb-1\">{previewSlide.emoji}</span>\n            )}\n            <h4\n              className={`font-bold text-[10px] leading-tight line-clamp-2 ${getTextColorClass(template)}`}\n            >\n              {previewSlide.headline}\n            </h4>\n          </div>\n        )}\n\n        {/* Template Info */}\n        <div className=\"space-y-1\">\n          <div className=\"flex items-center justify-between\">\n            <h3 className=\"font-semibold text-sm\">{template.name}</h3>\n            {isSelected && <Check className=\"w-4 h-4 text-primary\" />}\n          </div>\n          <p className=\"text-xs text-muted-foreground line-clamp-1\">\n            {template.description}\n          </p>\n          <span className=\"inline-block text-[10px] px-1.5 py-0.5 bg-muted rounded capitalize\">\n            {template.category}\n          </span>\n        </div>\n      </div>\n    </Card>\n  );\n}\n\nexport function TemplateGallery() {\n  const { selectedTemplateId, setSelectedTemplateId, filterCategory, setFilterCategory } =\n    useTemplateStore();\n\n  const filteredTemplates = getTemplatesByCategory(filterCategory);\n\n 